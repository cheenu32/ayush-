6th question==


#include <stdio.h>
#include <math.h>

int main()
{
    float a;
    printf("enter the value of a\n");
    scanf("%f",&a);
    
    
    printf("diameter of circle:=%f\n", 2*a);
    printf("circumference of circle:=%f\n", 2*3.14*a);
    printf("area of circle:=%f\n", 3.14*a*a);
    
    

    return 0;
}

7th question==

#include <stdio.h>
#include <math.h>

int main()
{
    float a;
    printf("enter the value of a\n");
    scanf("%f",&a);
    
    
    printf("cm to m:=%f\n", a/100);
    printf("cm to km:=%f\n", a/1000);

    
    

    return 0;
}

