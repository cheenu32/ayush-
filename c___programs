}

9th question=

#include <stdio.h>
#include <math.h>

int main()
{
    float a;
    printf("enter the value of a\n");
    scanf("%f",&a);
    
    
    printf("faren to cel:=%f\n", ((a-32)*5)/9);
    return 0;
}

10 question==


#include <stdio.h>
#include <math.h>

int main()
{
    int a,b,c,d;
    printf("enter the value of a\n");
    scanf("%d",&a);
    
    b=a/365;
    c=(a%365)/7;
    d=((a%365)%7);
    printf("year:=%d\n",b);
    printf("weeks:=%d\n",c);
    printf("days:=%d\n",d);
    return 0;
}
