1st question from 18 questions:-

#include <stdio.h>

int main()
{
    char a;
    int b;
    float f;
    double d;
    scanf("%c",&a);
    scanf("%d",&b);
    scanf("%f",&f);
    scanf("%lf",&d);
    
    printf("%c\n",a);
    printf("%d\n",b);
    printf("%f\n",f);
    printf("%lf\n",d);

    return 0;
}

2nd question from 18 questions:-


#include <stdio.h>

int main()
{
    int a,b,sum;
    printf("enter the value of a&b\n");
    scanf("%d%d",&a,&b);
    sum=a+b;
    printf("sum=%d",sum);

    return 0;
}


 3rd question:- 

#include <stdio.h>

int main()
{
    int a,b,sum,multi,di,subs;
    printf("enter the value of a&b\n");
    scanf("%d%d",&a,&b);
    
    sum=a+b;
    printf("sum=%d\n",sum);
    subs=a-b;
    printf("substraction=%d\n",subs);
    multi=a*b;
    printf("multiplication=%d\n",multi);
    di=a/b;
    printf("divison=%d\n",di);
    

    return 0;
}

